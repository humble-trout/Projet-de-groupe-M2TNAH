--cr√©ation des tables de relation

create table calendrier
(
	id_utilisateur INTEGER NOT NULL,
	id_seance INTEGER,
	FOREIGN KEY (id_utilisateur) REFERENCES utilisateur(id_utilisateur),
	FOREIGN KEY (id_seance) REFERENCES seance(id_seance)
);


