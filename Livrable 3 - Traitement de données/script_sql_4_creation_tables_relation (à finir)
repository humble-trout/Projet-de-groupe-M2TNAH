create table cinema_favori
(
	id_utilisateur INTEGER NOT NULL,
	id_cinema INTEGER,
	FOREIGN KEY (id_utilisateur) REFERENCES utilisateur(id_utilisateur),
	FOREIGN KEY (id_cinema) REFERENCES cinema(id_cinema)
